<html lang="en">
	<head>
		<title>{{title}}</title>
		{% block head %}
		<link rel="stylesheet" href="{{static}}/css/lib/bootstrap.min.css">
		{% endblock %}
	</head>
	<body>
		<div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
			<div class="container">
				<div class="navbar-header">
					<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
						<span class="sr-only">Toggle navigation</span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</button>
					<a class="navbar-brand" href="/" title="{{proj_tagline}}">{{proj_name}}</a>
				</div>
				<div class="navbar-collapse collapse">
					{# <ul class="nav navbar-nav">
						<li><a href="#">Dashboard</a></li>
						<li><a href="#">Settings</a></li>
						<li><a href="#">Profile</a></li>
						<li><a href="#">Help</a></li>
					</ul> #}
					{% if user_data %}
						<ul class="nav navbar-nav navbar-right">
							<li>
								<p class="navbar-text">
									Logged in as 
									<a href="{{site_url('/dashboard')}}" title="Profile">
										{{user_data.login}}
									</a>
								</p>
							</li>
							<li>
								<a href="{{site_url('/logout')}}" title="logout">Logout</a>
							</li>
						</ul>
					{% elseif not hidelogin %}
						{% include "partial/loginform.p.html.twig" %}
					{% endif %}
				</div><!--/.navbar-collapse -->
			</div>
		</div>

		{% block jumbotron %}
		{% endblock %}

		<div class="container" style="margin-top: 51px;">
		{{messages}}
		{% block content %}
			There doesn't seem to be any content on this page...
		{% endblock %}
			<hr>

			<footer>
				<p>{{proj_name_abbr}} &copy; {{proj_company}} {{"now"|date("Y")}}</p>
			</footer>
		</div> <!-- /container -->

		{% block foot %}
		<script type="text/javascript" src="{{static}}/js/lib/jquery-1.11.0.min.js"></script>
		<script type="text/javascript" src="{{static}}/js/lib/bootstrap.min.js"></script>
		{% endblock %}
	</body>
</html>